          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "(1 - node_filesystem_avail_bytes{instance=\"192.168.0.12:9100\",mountpoint=\"/\",fstype!=\"tmpfs\"} / node_filesystem_size_bytes{instance=\"192.168.0.12:9100\",mountpoint=\"/\",fstype!=\"tmpfs\"}) * 100",
