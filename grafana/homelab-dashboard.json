    {
      "datasource": { "type": "influxdb", "uid": "${DS_INFLUXDB}" },
      "description": "InfluxDB query for Unraid CPU via Telegraf. Change the bucket name if yours differs.",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          },
          "unit": "percent"
